{
  "version": 2,
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ],
  "functions": {
    "index.py": {
      "maxDuration": 30
    }
  }
}
